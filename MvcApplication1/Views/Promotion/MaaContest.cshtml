    <div class="covered_form">
    <div class="err_msg" id="diverrmsg">Error: Please check the values and try again</div>
    <div class="enquiry_form">
    	<p>
        	<label><span class="star">*</span>Full Name:</label>
            <input id="txtFullName" name="txtFullName" type="text" class="input_txt" />
        </p>
        <p>
        	<label><span class="star">*</span>Email:</label>
            <input id="txtEmail" name="txtEmail" type="text" class="input_txt" />
        </p>
        <p>
        	<label><span class="star">*</span>Phone:</label>
            <input id="txtPhone" name="txtPhone" type="text" class="input_txt" />
        </p>
        <p>
        	<label>City:</label>
            <input id="txtCity" name="txtCity" type="text" class="input_txt" />
        </p>
        <p>
        	<label>Zip Code:</label>
            <input id="txtZipCode" name="txtZipCode" type="text" class="input_txt" />
        </p>
    </div>
    <div class="enquiry_form">
    	<p>
        	<label><span class="star">*</span>Your Message: <span class="msg_txt">(Send a loving message to your mom in just 10 words)</span></label>
            <textarea id="txtMessage" name="txtMessage" class="input_area"></textarea>
        </p>
        <p>
        	<label>Address:</label>
            <input id="txtAddress" name="txtAddress" type="text" class="input_txt" />
        </p>
        <p>
        	<label>State:</label>
            <input id="txtState" name="txtState" type="text" class="input_txt" />
        </p>
    </div>
