                                success: function (data) {
                                    predata = data;

                                    $.each(data, function () {
                                        jsonData.push({ id: this.Id, cname: this.CountryFlag, name: this.Name, Code: this.CountCode, status: 'Already visited' });
                                    });

                                    var lowestrate = $('#ms21').magicSuggest({
                                        displayField: 'cname',
                                        allowFreeEntries: false,
                                        data: jsonData,
                                        resultAsString: true,
                                        maxSelection: 1,
                                        strictSuggest: true,

                                        renderer: function (data) {
                                            return '<div>' +
                                                data.cname + '(+' + data.Code + ')' +
                                                '</div><div style="clear:both;"></div>';
                                        },
                                        maxSelectionRenderer: function () {
                                        }
                                    });

                                    $(lowestrate).on('selectionchange', function (event, selection) {
                                        var selected = lowestrate.getSelectedItems();
                                        var countryto = selected[0].id;

                                        var countryfrom = $("#global-countries_17").val();
                                        window.location.href = "/Rate/SearchRate?countryfrom=" + countryfrom + "&countryto=" + countryto + "";
                                        //lowestRateInfoViewModel.LowestRate(countryfrom, countryto);
                                    });

                                }
                            });

                            //var countryid = $("#country-byip").val();
                            //setSelectedIndex(document.getElementById("stamp_flag"), countryid);

                        });
                    </script>

