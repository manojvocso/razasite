<script src="@Url.Content("~/Scripts/magicsuggest-1.3.1.js")"></script>
<script>
    $(document).ready(function () {

        var jsonData = [];
        var cities = '<a><img src="/images/pakistan.png" alt="">Pakistan (+92)</a>'.split(',');
        for (var i = 0; i < cities.length; i++) jsonData.push({ id: i, name: cities[i], status: i % 2 ? 'Already Visited' : 'Planned for visit', coolness: Math.floor(Math.random() * 10) + 1 });


        var ms7 = $('#ms-input').magicSuggest({
            data: jsonData,
            resultAsString: true,
            maxSelection: 1,
            value: [0],
            maxSelectionRenderer: function () { }
        });

    });

</script>

