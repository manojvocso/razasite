<script type="text/javascript">
    (function ($, undefined) {
        var ie = $.browser.msie;
        var tabbarLi = $(".tabbar-mobile-select li");
        var tabbarLiFirst = $(".tabbar-mobile-select li:first");
        var tabbarLiActive = $(".tabbar-mobile-select li.active");
        $(".pane").hide();
        tabbarLiFirst.addClass("active").show();
        $(".pane:first").show();
        tabbarLi.append('<div class="on"></div>');
        tabbarLi.append('<div class="off"></div>');
        tabbarLiActive.find(".off").hide();
        tabbarLiActive.find('.off').css('top', '0px');
        tabbarLi.mouseover(function () {
            if ((!$(this).hasClass('active')) && (!$(this).hasClass('disabled'))) {
                $(this).find(".off").clearQueue();
                $(this).find(".off").animate({ top: '0px' }, 200, 'swing');
            }
        });
        tabbarLi.mouseout(function () {
            if ((!$(this).hasClass('active')) && (!$(this).hasClass('disabled'))) {
                $(this).find(".off").clearQueue();
                $(this).find(".off").animate({ top: '68px' }, 200, 'swing');
            }
        });
        tabbarLi.click(function () {
            if ((!$(this).hasClass('active')) && (!$(this).hasClass('disabled'))) {
                var aActive = $(this).find("a").attr("href");
                var offDiv = $(this).find(".off");
                var onDiv = $(this).find(".on");
                var oldOffDiv = $(".tabbar-mobile-select li.active .off");
                $(oldOffDiv).show();
                $(oldOffDiv).animate({ top: '68px' }, 150, 'swing');
                $(".tabbar-mobile-select li .on").hide();
                tabbarLi.removeClass("active");
                $(this).addClass("active");
                $(onDiv).css('top', '0px');
                $(".pane").hide();
                $(onDiv).show();
                $(aActive).fadeIn(300);
                $(offDiv).fadeOut(100);
                if (ie) {
                    $(aActive).find(".ticket").hide().fadeIn(300);
                    $(aActive).find(".ih4").hide().fadeIn(300);
                }
            }
            return false;
        });
        jQuery.fn.fadeIn = function (speed, callback) {
            return this.animate({ opacity: 'show' }, speed, function () {
                if (jQuery.browser.msie)
                    this.style.removeAttribute('filter');
                if (jQuery.isFunction(callback))
                    callback();
            });
        };
        jQuery.fn.fadeOut = function (speed, callback) {
            return this.animate({ opacity: 'hide' }, speed, function () {
                if (jQuery.browser.msie)
                    this.style.removeAttribute('filter');
                if (jQuery.isFunction(callback))
                    callback();
            });
        };
        // find the div.fade elements and hook the hover event
        $('div.fade').hover(function () {
            // on hovering over, find the element we want to fade up
            var fade = $('> div', this);
            // if the element is currently being animated (to a fadeOut)...
            if (fade.is(':animated')) {
                // ...take it's current opacity back up to 1
                fade.stop().fadeTo(150, 1);
            } else {
                // fade in quickly
                fade.fadeIn(150);
            }
        }, function () {
            // on hovering out, fade the element out
            var fade = $('> div', this);
            if (fade.is(':animated')) {
                fade.stop().fadeTo(400, 0);
            } else {
                // fade away slowly
                fade.fadeOut(400);
            }
        });
    })(RAZA.jQuery);
</script>