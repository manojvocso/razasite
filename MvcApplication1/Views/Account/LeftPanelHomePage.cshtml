	   <script type="text/javascript">
	        function showid(id) {
	            var el = document.getElementById(id).style;
	            el.display = "block";
	        }

	        function hideid(id) {
	            var el = document.getElementById(id).style;
	            el.display = "none";
	        }

	        function toggle(id) {
	            //display:none has to be in element and not inside class for it to work.
	            var el = document.getElementById(id).style;
	            if (el.display == "none") {
	                el.display = "block";
	            }
	            else if (el.display == "block") {
	                el.display = "none";
	            }
	        }

</script>	
		
