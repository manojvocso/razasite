	<script>

	      function show_form() {
	          var active_msg = 0;
	          $("#credit").slideDown("slow", function () {
	          
	              $(".deals-detail").mouseleave(function () {
	                  var self = this;
	                  active_msg = 1;
	                  //active_msg(active_msg,self);
	                  jQuery('#active_deal').val(active_msg);
	                  setTimeout(function () {

	                      active_msg = 0;
	                      jQuery('#active_deal').val(active_msg);

	                      active_close_msg();
	                  }, 0);



	              });//mouserleave



	          });

	      }

	      function active_close_msg() {

	          var active_deal1 = parseInt(jQuery('#active_deal').val());

	          if (active_deal1 == 0) {

	              setTimeout(function () {
	                  $(self).fadeOut(0);
	              }, 0);
	              $("#credit").slideUp("slow");

	          }
	      }
	      function close_form() {
	          $("#credit").slideUp("slow");

	      }


</script>
	
