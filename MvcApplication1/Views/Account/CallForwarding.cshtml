    <div id="feedback_panel">
        <div id="feedback-scroller">
            <div class="top-bg">
                <h1>
                    Call Forwarding Setup<span data-bind="text: Status" style="padding-left: 200px;font-size:14px;font-weight: bold;color:red"></span></h1>
            </div>
            <div class="repeat-scroller-bg">
                <div class="new_forwarding_set">
         <form class="tabs38-frombar" >
                    
                        <div class="clear">
                        </div>
                        <div class="spacer">
                        </div>
                        <fieldset class="odd oddest">
                            <input type="hidden" id="order_id" value="@ViewBag.order_id" name="order_id"/>
                            <label class="oddest_label">
                                Select Your 1-800 Number <font color="#FF0000">*</font></label>
                            <select id="Number_800" name="Number_800" data-bind="options: Number, optionsText: 'GetNumber', optionsValue: 'GetNumber', optionsCaption: 'Select Your 1-800 Number',value:number_800" >
                     
                            </select>
                        </fieldset>
                        <fieldset class="even oddest">
                            <label class="oddest_label">
                                Select Country <font color="#FF0000">*</font></label>
                            <select id="CountryTo" onchange="FetchContact(this);" name="CountryTo" data-bind="options: CountryTo, optionsText: 'Name', optionsValue: 'CountryCode', optionsCaption: 'Select Country',value:CountryCode">
                            </select>
                        </fieldset>
                        <script type="text/javascript">
                            function FetchContact(data) {
                                document.getElementById("CountryCode").value = data.value;
                            }
                                                        
                        </script>
                        <!--<fieldset class="odderes_new">
                            </fieldset>-->
                        <h3>Enter Destination Number</h3>
                        <fieldset  class="odd oddest ">
                            <label class="oddest_label">Country Code +  Phone Number <font color="#FF0000">*</font></label>
                            <input type="text"  class="ltr-text" id="CountryCode" name="CountryCode" readonly="readonly"  />
                            <input type="text"    onkeypress="return  numberOnly();"  data-bind="value:DestinationNum"  />
                        </fieldset>
                        <fieldset  class="even oddest">
                            <label class="oddest_label">Activation Date <font color="#FF0000">*</font></label>
                            <input type="text" value="" name="Activation_Date" id="Activation_Date"  class="date-pick dp-applied" />
                           (mm/dd/yy)
                            <div id="LoginPwd4" style="padding-left:50px;color: red;"></div>
                        </fieldset>
                        <fieldset  class="odd oddest">
                            <label class="oddest_label">Expiry Date <font color="#FF0000">*</font></label>
                     
                          <input type="text" value="" name="Expiry_Date" id="Expiry_Date"   class="date-pick dp-applied" />
                      (mm/dd/yy)
                            <div id="LoginPwd6" style="padding-left:70px;color: red;"></div>
                         
                        </fieldset>
                        <fieldset  class="even oddest">
                            <label class="oddest_label">Name Your Call Forwarding Number </label>
                            <input type="text"  data-bind="value:Forwarding_Name"/>
                        </fieldset>
              
             <fieldset  class="odd oddest">
                    
                            <input type="button" value="" class="create-btn oddest" id="CallFrowrdbtn"  />
                            <div class="showdiv" >
                                <div class="clear"></div>
                                <div class="spacer"></div>
                             <table class="assignback" cellspacing="0" cellpadding="0" width="100%" border="0">
                                 <tbody align="center">
                                     <tr>
                                         <td class="tableheader" colspan="8" align="center">Your call forwarding setup</td>
                                     </tr>
                                     <tr>
                                         <td width="50" id="subhead_small"><strong>Sno</strong></td>
                                         <td width="125" id="subhead_small" style="width:140px;"><strong>Callforwarding Number</strong></td>
                                         <td width="121" id="subhead_small"><strong>Destination Number</strong></td>
                                         <td width="101" id="subhead_small"><strong>Activation Date</strong></td>
                                         <td width="101" id="subhead_small"><strong>Expiry Date</strong></td>
                                         <td width="100" id="subhead_small"><strong>Name</strong></td>
                                         <td width="45" id="subhead_small"><strong>Status</strong></td>
                                         <td width="30" id="subhead_small"><strong>Delete</strong></td>

                                     </tr>
                               
                                     <tr> </tr>
                                 </tbody>
                                 <tbody data-bind="foreach:CallForwardList" style="height:15px;" >
                               <tr>
                                            <td width="50" align="center"  data-bind="text:($index()+1)"></td>
                                    <td width="81" align="center"  data-bind="text:FollowMeNumber"></td>
                                            <td width="131" align="center"  data-bind="text:DestNum"></td>
                                            <td width="131" align="center"   data-bind="text:ActiveDate"></td>
                                            <td width="132" align="center"   data-bind="text:ExpDate" ></td>
                                            <td width="117"  align="center"  data-bind="text:Name"></td>
                                            <td width="117" align="center"   data-bind="text:Status"></td>
                      <td width="52" align="center" ><a id="delete"  data-bind="click:$root.loadpopupval" style="cursor:pointer;"><img src="@Url.Content("~/images/new_delete_icon.png")"  alt="Delete" /></a></td>
          

     </tr>
                 </tbody>
                </table>
                 </div>
                   
         </fieldset>
                    </form>
