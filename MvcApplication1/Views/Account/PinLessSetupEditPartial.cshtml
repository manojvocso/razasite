                        @if (Model.PinlessNumberList != null)
                        {
                            int count = 1;
                            foreach (var i in Model.PinlessNumberList)
                            {
                                string trid = i.PinlessNumber;
                                <tr>

                                    <td height="33" bgcolor="#f5f4e9" class="white">Phone# @count.</td>

                                    <td align="right" style="font-size: 17px; margin-left: 10px;" height="33" bgcolor="#f5f4e9" class="white phone_hex"> @i.PinlessNumber</td>

                                    <td height="33" bgcolor="#f5f4e9" class="white">&nbsp;</td>

                                    <td align="center" height="33" align="center" bgcolor="#f5f4e9" class="white">

                                      <a id="delete" class="ajaxAction" href="@Url.Action("DeletePinLessSetup", new { pn = i.PinlessNumber, cd = i.CountryCode, oid = Model.OrderID })"><img src="@Url.Content("~/images/new_delete_icon.png")" alt="Delete" /></a>

                                    </td>

                                </tr>
                                count++;
                            }
                        }

